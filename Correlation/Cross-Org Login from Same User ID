source="prod" OktaClass=PasswordBasedLoginController "LoginStatus: SUCCESS" 
     AND user!="delauth" 
     AND user!="user@ok1-ops-mon.okta.com" 
     AND user!="delauth@admon-ok1.admon.saasure.com" 
     AND user!="user@ok2-ops-mon.okta.com" 
     AND user!="svc_solar" 
     AND user!="delauth@admon-ok2.admon.saasure.com" 
| rex field=_raw "User: \[(?<usr>.*)\] attempted login on domain \[(?<domain>.*)\] from \[(?<sip>.*)\] with result:" 
| stats values(org) as Org_Id values(domain) as Organization dc(org) as Org_count by user 
| sort -Org_count 
| where Org_count > 1
